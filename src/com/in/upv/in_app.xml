<?xml version="1.0" encoding="UTF8"?>
<Matches>
	
	
	<!--启动APP-->	
	<stat_unit>
		<action>position</action>
		<description>获取服务器发送的时间戳</description>
		<stat_type>daily</stat_type>
		<match combine_type="and">                                                             
			<entry field_name="request" match_type="contain"><![CDATA[/timestamp.php]]></entry>
		</match>
	</stat_unit>
	
	<stat_unit>
		<action>position</action>
		<description>APNS</description>
		<stat_type>daily</stat_type>
		<match combine_type="and">                                                             
			<entry field_name="request" match_type="contain"><![CDATA[/api/apns]]></entry>
		</match>
	</stat_unit>
	
	<stat_unit>
		<action>position</action>
		<description>获取贴纸剩余次数</description>
		<stat_type>daily</stat_type>
		<match combine_type="and">                                                             
			<entry field_name="request" match_type="contain"><![CDATA[/app/paster/leftcount]]></entry>
		</match>
	</stat_unit>
	
	<stat_unit>
		<action>position</action>
		<description>查看任务完成情况</description>
		<stat_type>daily</stat_type>
		<match combine_type="and">                                                             
			<entry field_name="request" match_type="contain"><![CDATA[/app/task/status]]></entry>
			<entry field_name="refer" match_type="contain"><![CDATA[/app/task/status]]></entry>
			<entry field_name="hostname" match_type="contain"><![CDATA[/app/task/status]]></entry>
			<entry field_name="request" match_type="contain"><![CDATA[/app/task/status]]></entry>
		</match>
	</stat_unit>
	
	<!--进入首页（默认进入关注页面）-->
