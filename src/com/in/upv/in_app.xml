  	<!--获取贴纸剩余次数-->
	<stat_unit>
		<action>position</action>
		<description>获取贴纸剩余次数</description>
		<pvitem>8058</pvitem>
		<uvitem>8059</uvitem>
		<stat_type>daily</stat_type>
		<match combine_type="and">                                                             
			<entry field_name="request" match_type="contain"><![CDATA[/shopping/mobile]]></entry>
		</match>
	</stat_unit>
		
	
</Matches>