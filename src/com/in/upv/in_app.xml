		<action>position</action>
		<description>获取关注页面数据</description>
		<stat_type>daily</stat_type>
		<match combine_type="and">                                                             
			<entry field_name="request" match_type="contain"><![CDATA[/app/home]]></entry>
		</match>
	</stat_unit>
	
